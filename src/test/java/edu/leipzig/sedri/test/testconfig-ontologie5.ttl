             sedri:param "class"^^xsd:string ;
             sedri:postprocessor "edu.leipzig.sedri.test.TestPostprocessor"^^xsd:string ;
             sedri:preprocessor "edu.leipzig.sedri.test.WrongTestPreprocessor"^^xsd:string ;
             sedri:sources :SPARQLNode8Source1, :SPARQLNode8Source2 .

:SPARQLNode8Source1 rdf:type sedri:Source ;
         sedri:sourceURL "http://dbpedia.org/sparql"^^xsd:string ;
         sedri:sparqlQuery "select ?s where {?s a <http://dbpedia.org/ontology/$class>} order by ?s limit 1"^^xsd:string .

