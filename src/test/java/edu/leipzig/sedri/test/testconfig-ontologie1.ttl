@prefix : <urn:uuid:testconfig01#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sedri: <http://sedri.de/vocab/> .
@base <urn:uuid:testconfig01> .

#################################################################
#
#    Webserver1 corresponding testconfig1.xml
#
#################################################################

:Webservice1 rdf:type sedri:Webservice ;
             sedri:portNumber 1234 ;
             sedri:nodes :SPARQLNode1 .

:SPARQLNode1 rdf:type sedri:SPARQLNode ;
             sedri:url-path "/test"^^xsd:string ;
             sedri:resultFormat "RDF/XML"^^xsd:string ;
             sedri:param "class"^^xsd:string ;
             sedri:postprocessor "edu.leipzig.sedri.test.TestPostprocessor"^^xsd:string ;
             sedri:preprocessor "edu.leipzig.sedri.test.TestPreprocessor"^^xsd:string ;
             sedri:sources :source1 , :source2 .

:source1 rdf:type sedri:Source ;
         sedri:sourceURL "http://dbpedia.org/sparql"^^xsd:string ;
         sedri:sparqlQuery "select ?s where {?s a <http://dbpedia.org/ontology/$class>. FILTER regex(?s, \"Methylfentanyl\")} order by ?s limit 1"^^xsd:string .

:source2 rdf:type sedri:Source ;
         sedri:sourceURL "http://drugbank.bio2rdf.org/sparql"^^xsd:string ;
         sedri:sparqlQuery "select ?s where {?s a <http://bio2rdf.org/drugbank_vocabulary:Drug>} order by ?s limit 1"^^xsd:string .
