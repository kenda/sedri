:Webservice2 rdf:type sedri:Webservice ;
             sedri:portNumber 5678 ;
             sedri:nodes :SPARQLNode2 , :SPARQLNode3 .

:SPARQLNode2 rdf:type sedri:SPARQLNode ;
             sedri:url-path "/test1"^^xsd:string ;
             sedri:resultFormat "JSON-LD"^^xsd:string ;
             sedri:preprocessor "edu.leipzig.sedri.test.TestPreprocessor"^^xsd:string ;
             sedri:sources :source3 .

:source3 rdf:type sedri:Source ;
		 sedri:sourceURL "http://drugbank.bio2rdf.org/sparql"^^xsd:string .

:SPARQLNode3 rdf:type sedri:SPARQLNode ;
             sedri:url-path "/test2/test3"^^xsd:string ;
             sedri:resultFormat ""^^xsd:string ;
             sedri:param "testLimit"^^xsd:string ;
             sedri:param "testLimit"^^xsd:string ;
             sedri:postprocessor "edu.leipzig.sedri.test.TestPostprocessor"^^xsd:string .