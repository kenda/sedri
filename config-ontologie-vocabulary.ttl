###  http://www.sedri.de/vocab/param

:param rdf:type owl:DatatypeProperty ;
       
       rdfs:label "URL-GET Parameter"@de ,
                  "URL-GET parameter"@en ;
       
       rdfs:comment "Name eines erwarteten URL GET-Parameters für die dynamische Query-Erzeugung."@de ,
                    "Name of an expected URL GET parameter to generate dynamic queries."@en ;
       
       rdfs:domain :Node ;
       
       rdfs:range xsd:string .



###  http://www.sedri.de/vocab/portNumber

:portNumber rdf:type owl:DatatypeProperty ;
            
            rdfs:label "Portnummer"@de ,
                       "Portnumber"@en ;
            
            rdfs:comment "Die Portnummer unter welcher der Webservice erreichbar ist."@de ,
                         "The port number where the webservice is accessable."@en ;
            
            rdfs:domain :Webservice ;
            
            rdfs:range xsd:integer .



###  http://www.sedri.de/vocab/postprocessor

:postprocessor rdf:type owl:DatatypeProperty ;
               
               rdfs:label "Post-Processor Klassenname"@de ,
                          "Post-Processor classname"@en ;
               
               rdfs:comment "Der Klassenname eines Post-Processors um das Ergebnis der Datenaggregation vor der Ergebnisausgabe zu verändern."@de ,
                            "The classname of a post processor that manipulates the aggregated data before the result is returned."@en ;
               
               rdfs:domain :Node ;
               
               rdfs:range xsd:string .



###  http://www.sedri.de/vocab/preprocessor

:preprocessor rdf:type owl:DatatypeProperty ;
              
              rdfs:label "Pre-Processor Klassenname"@de ,
                         "Pre-Processor classnames"@en ;
              
              rdfs:comment "Der Klassenname eines Pre-Processors der z.Bsp. die URL-GET Paramterliste des Knotepunktes vor der Datenanfrage verändert."@de ,
                           "The classname of a pre processor that may manipulate the URL-GET parameter list before the data aggregation process."@en ;
              
              rdfs:domain :Node ;
              
              rdfs:range xsd:string .



###  http://www.sedri.de/vocab/resultFormat

:resultFormat rdf:type owl:DatatypeProperty ;
              
              rdfs:label "Ergebnisformat"@de ,
                         "result format"@en ;
              
              rdfs:comment "Legt das Ergbnisformat eines SPARQL-Kontens fest. Mögliche Formate sind TURTLE, N-TRIPLE, RDF/XML und JSON-LD."@de ,
                           "Set the result fomrat of a SPARQL node. Possible formats are TURTLE, N-TRIPLE, RDF/XML and JSON-LD."@en ;
              
              rdfs:domain :SPARQLNode ;
              
              rdfs:range xsd:string .



###  http://www.sedri.de/vocab/sourceIndex

:sourceIndex rdf:type owl:DatatypeProperty ;
             
             rdfs:label "Quellenindex"@de ,
                        "Sourceindex"@en ;
             
             rdfs:comment "Quellenindex um eine Reihenfolge der Quellzugriffspunkte zu konfigurieren."@de ,
                          "Index of the source endpoint to configure a order."@en ;
             
             rdfs:domain :Source ;
             
             rdfs:range xsd:integer .



###  http://www.sedri.de/vocab/sourceURL

:sourceURL rdf:type owl:DatatypeProperty ;
           
           rdfs:label "URL einer Quelle, z.Bsp. eines SPARQL-Endpoints."@de ,
                      "URL of a source, e.g. a SPARQL endpoint."@en ;
           
           rdfs:comment "Repräsentiert die URL-Adress einer Quelle, z.Bsp. eines SPARQL Endpoints."@de ,
                        "Represents the URL adress of a source, e.g. a SPARQL endpoint."@en ;
           
           rdfs:domain :Source ;
           
           rdfs:range xsd:string .



###  http://www.sedri.de/vocab/sparqlQuery

:sparqlQuery rdf:type owl:DatatypeProperty ;
             
             rdfs:label "Vorkonfiguriertes SPARQL Query"@de ,
                        "preconfigured SPARQL query"@en ;
             
             rdfs:comment "Repräsentiert ein SPARQL Query mit den definierten URL-GET Parametern."@de ,
                          "Represent a SPARQL query with the preconfigured URL-GET paramter."@en ;
             
             rdfs:domain :Source ;
             
             rdfs:range xsd:string .



###  http://www.sedri.de/vocab/url-path

:url-path rdf:type owl:DatatypeProperty ;
          
          rdfs:label "URL-Pfad"@de ,
                     "URL path"@en ;
          
          rdfs:comment "Relativer URL-Pfad unter dem der Knotenpunkt erreichbar sein soll."@de ,
                       "Relative URL path where the node will be accessable."@en ;
          
          rdfs:domain :Node ;
          
          rdfs:range xsd:string .





